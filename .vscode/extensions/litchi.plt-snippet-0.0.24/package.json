{
	"name": "plt-snippet",
	"displayName": "Matplotlib Pilot",
	"description": "matplotlib代码片段生成",
	"icon": "media/matplotlibpilot.t.logo.png",
	"version": "0.0.24",
	"publisher": "litchi",
	"engines": {
		"vscode": "^1.72.0"
	},
	"categories": [
		"Data Science"
	],
	"keywords": [
		"matplotlib",
		"snippet",
		"template",
		"jupyter notebook",
		"matplotlib style"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "sidebar",
					"title": "PLT",
					"icon": "media/matplotlibpilot.logo.svg"
				}
			]
		},
		"views": {
			"sidebar": [
				{
					"type": "webview",
					"id": "pltSidebarView",
					"name": "",
					"dataProvider": "pltSidebarProvider"
				}
			]
		},
		"dataProviders": [
			{
				"id": "pltSidebarProvider",
				"provides": "pltSidebarView"
			}
		],
		"commands": [
			{
				"command": "plt-snippet.loadtxt",
				"category": "Plt",
				"title": "loadtxt"
			},
			{
				"command": "plt-snippet.load",
				"category": "Plt",
				"title": "load"
			},
			{
				"command": "plt-snippet.defineCustomTemplate",
				"category": "Plt",
				"title": "define custom template"
			}
		],
		"configuration": {
			"title": "Plt Snippet",
			"properties": {
				"plt-snippet.fontPath": {
					"type": "string",
					"default": "C:\\Users\\username\\Documents\\fonts\\path_to_font.ttf",
					"description": "本地字体路径(内测阶段)"
				},
				"plt-snippet.customTemplateHtml": {
					"type": "string",
					"default": "",
					"description": "自定义模板网页"
				},
				"plt-snippet.commentWhenClick": {
					"type": "boolean",
					"default": false,
					"description": "单击按钮粘贴使用带有注释的代码片段"
				},
				"plt-snippet.commentWhenDoubleClick": {
					"type": "boolean",
					"default": true,
					"description": "双击按钮粘贴使用带有注释的代码片段"
				},
				"plt-snippet.tutorialBotton": {
					"type": "boolean",
					"default": false,
					"description": "是否显示教程按钮（重启生效）"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@babel/core": "^7.24.5",
		"@babel/preset-env": "^7.24.5",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@types/vscode": "^1.72.0",
		"@typescript-eslint/eslint-plugin": "^7.4.0",
		"@typescript-eslint/parser": "^7.4.0",
		"@vscode/test-cli": "^0.0.8",
		"@vscode/test-electron": "^2.3.9",
		"babel-loader": "^9.1.3",
		"copy-webpack-plugin": "^12.0.2",
		"eslint": "^8.57.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.3.3",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nfvm/plt-snippet"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.12"
	},
	"__metadata": {
		"id": "99bbea87-70b1-4968-be1d-b4262722e6d0",
		"publisherId": "94d53034-b0e6-44d5-9753-3c78cfc157b9",
		"publisherDisplayName": "litchi",
		"targetPlatform": "undefined",
		"isApplicationScoped": false,
		"isPreReleaseVersion": false,
		"hasPreReleaseVersion": false,
		"installedTimestamp": 1724527702571,
		"pinned": false,
		"preRelease": false,
		"source": "gallery"
	}
}